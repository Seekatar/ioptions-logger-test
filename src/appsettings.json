    "Properties": {
      "Application": "TestApp"
    }
  },

  // loose values
  // will be overridden by environment 
  "FromEnvironment": "AppSettings",
  // will be overridden by appsettings.Development.json
  "FromDevelopmentSettings": "AppSettings",
  "FromAppSettings": "AppSettings1",

  // section values
  "Configuration": {
    // will be overridden by environment 
    "FromEnvironment": "AppSettings",
    // will be overridden by appsettings.Development.json
    "FromDevelopmentSettings": "AppSettings",

    "FromAppSettings": "AppSettings2"
  },

  "Monitored": {
    "FromAppSettings": "AppSettings3"
  },
  "Snapshot": {
    "FromAppSettings": "AppSettings4"
  },
  "OneTime": {
    "FromAppSettings": "AppSettings5"
  },

  "AllowedHosts": "*"
}
